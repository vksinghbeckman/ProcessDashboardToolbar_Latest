             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d" d:DesignWidth="300"
             Name="MyToolWindow" Height="299.415">
    <Grid>
        <Grid Margin="10,-39,0,38.2" >

            <Button Content="Finish" Click="button1_Click" x:Name="button1" Margin="34,253,138,22"/>
            <Button x:Name="button2" Content="Pause" Height="27" Margin="155,133,25,140" RenderTransformOrigin="0.168,0.372"/>
            <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Height="29" Margin="32,133,0,138" Width="49"/>
            <CheckBox x:Name="checkBox" Content="CheckBox" Margin="34,217,128,67"/>
            <ComboBox x:Name="comboBox" Margin="32,181,111,102"/>
        </Grid>
    </Grid>
</UserControl>
